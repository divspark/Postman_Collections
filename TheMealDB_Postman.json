            "url": {
              "raw": "{{base_url}}/search.php?s=chicken",
              "protocol": "https",
              "host": ["www", "themealdb", "com"],
              "path": ["api", "json", "v1", "1", "search.php"],
              "query": [
                {
                  "key": "s",
